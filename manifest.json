{
  "manifest_version": 3,
  "name": "Reloj Grande",
  "description": "Aplicacion para un New Tab con reloj desde local, no necesita internet",
  "version": "1.0",
  "permissions": ["activeTab"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}
